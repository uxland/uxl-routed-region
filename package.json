{
  "name": "@uxland/uxl-routed-region",
  "version": "0.0.0-development",
  "description": "UXLand Routed Region",
  "homepage": "https://github.com/uxland/uxl-routed-region#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/uxland/uxl-routed-region.git"
  },
  "bugs": {
    "url": "https://github.com/uxland/uxl-routed-region/issues"
  },
  "author": "UXLand <dev@uxland.es> (http://www.uxland.es)",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "polymer",
    "web components"
  ],
  "devDependencies": {
    "@olavoparno/jest-badges-readme": "^1.4.1",
    "@polymer/iron-demo-helpers": "^3.0.0-pre.19",
    "@polymer/iron-pages": "^3.0.0-pre.21",
    "@types/jest": "^24.9.1",
    "@types/ramda": "^0.27.4",
    "@types/redux-mock-store": "^1.0.2",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-when": "^2.7.0",
    "jsdom": "^15.2.1",
    "mutation-observer": "^1.0.3",
    "nsp": "^2.6.3",
    "raf": "^3.4.0",
    "redux-mock-store": "git+https://github.com/dpinart/redux-mock-store.git",
    "semantic-release": "^15.14.0",
    "travis-deploy-once": "^5.0.7",
    "ts-jest": "^24.3.0",
    "typescript": "^3.8.2"
  },
  "dependencies": {
    "@uxland/lit-redux-connect": "^2.0.0",
    "@uxland/uxl-redux": "^4.0.0",
    "@uxland/uxl-regions": "^4.0.0",
    "@uxland/uxl-routing": "^3.0.7",
    "@uxland/uxl-utilities": "^4.0.0",
    "ramda": "^0.27.0",
    "redux": "^4.0.5",
    "tslib": "^1.11.0"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:es": "tsc -p tsconfig.build.json --outDir es  ",
    "build:cjs": "tsc -p tsconfig.build.json --outDir lib --module commonjs --declaration",
    "build": "npm run build:es && npm run build:cjs",
    "test:unit": "jest --collect-coverage",
    "test": "npm run test:unit",
    "tdd": "jest --watch",
    "dist": "npm run build",
    "travis-deploy-once": "travis-deploy-once",
    "readme:coverage": "node_modules/.bin/jest-badges-readme",
    "semantic-release": "semantic-release"
  },
  "directories": {
    "test": "test"
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:unit && npm run readme:coverage && git add ."
    }
  }
}
